<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <context-param>
        <description>数据库配置文件路径</description>
        <param-name>dbconfig</param-name>
        <param-value>/WEB-INF/skyatom/skyatom_dbconfig.xml</param-value>
    </context-param>
    <context-param>
        <description>实体配置文件路径</description>
        <param-name>beanfile</param-name>
        <param-value>/WEB-INF/skyatom/_objbean.xml</param-value>
    </context-param>
    <context-param>
        <description>数据表配置文件路径</description>
        <param-name>dbfile</param-name>
        <param-value>/WEB-INF/skyatom/_dbbean.xml</param-value>
    </context-param>
    <context-param>
        <description>sql语句配置文件路径</description>
        <param-name>stpath</param-name>
        <param-value>/WEB-INF/skyatom/st/</param-value>
    </context-param>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
